{
    "name": "twedoo/volcator",
    "description": "Twedoo Volcator to build applications fast, comes with multiples modules configurable, that provides modern content management system (CMS).",
    "license": "MIT",
    "authors": [
        {
            "name": "Houssem MAAMRIA",
            "email": "houssem.maamria@twedoo.com",
            "homepage": "https://twedoo.com"
        }
    ],
    "homepage": "https://github.com/twedoo/volcator",
    "keywords": ["Laravel", "Volcator", "CMS", "Multi-Application", "Multi-Spaces"],
    "require": {
        "php": "^7.2|^8.0",
        "symfony/yaml": "^5.4",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "laravelcollective/html": "^6.2",
        "brian2694/laravel-toastr": "^5.56",
        "darkaonline/l5-swagger": "^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "predis/predis": "^2.0",
        "symfony/yaml": "^6.0.12",
        "pusher/pusher-php-server": "~7.0.2", 
        "beyondcode/laravel-websockets": "^1.13",
        "guzzlehttp/guzzle": "^7.0.1",
        "gazsp/baum": "^2.0",
        "twedoo/volcatorguard": "2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9"
    },
    "autoload": {
        "classmap": [
            "../../../app/Modules",
            "src/Modules",
            "commands",
            "database/seeders"
        ],
        "psr-4": {
            "Twedoo\\Volcator\\": "src/",
            "Modules\\": "src/Modules"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Twedoo\\Volcator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Twedoo\\Volcator\\VolcatorServiceProvider"
            ],
            "aliases": {
                "Entrust": "Zizaco\\Entrust\\EntrustFacade",
                "VolcatorEngine": "Twedoo\\Volcator\\Facades\\VolcatorEngineFacade",
                "VolcatorStructure": "Twedoo\\Volcator\\Facades\\VolcatorStructureFacade",
                "VolcatorMediaStyle": "Twedoo\\Volcator\\Facades\\VolcatorMediaStyleFacade",
                "VolcatorLanguage": "Twedoo\\Volcator\\Facades\\VolcatorLanguageFacade",
                "VolcatorMenu": "Twedoo\\Volcator\\Facades\\VolcatorMenuFacade",
                "VolcatorPath": "Twedoo\\Volcator\\Facades\\VolcatorPathFacade",
                "VolcatorFile": "Twedoo\\Volcator\\Facades\\VolcatorFileFacade",
                "VolcatorTranslation": "Twedoo\\Volcator\\Facades\\VolcatorTranslationFacade",
                "VolcatorApplication": "Twedoo\\Volcator\\Facades\\VolcatorApplicationFacade",
                "VolcatorSpace": "Twedoo\\Volcator\\Facades\\VolcatorSpaceFacade",
                "VolcatorInvitation": "Twedoo\\Volcator\\Facades\\VolcatorInvitationFacade",
                "VolcatorEmailNotification": "Twedoo\\Volcator\\Facades\\VolcatorEmailNotificationFacade",
                "VolcatorPushNotification": "Twedoo\\Volcator\\Facades\\VolcatorPushNotificationFacade"
            }
        }
    },
    "scripts": {
        "pre-autoload-dump": [
            "@php mkdir app/Modules"
        ],
        "post-autoload-dum": [
            "@php artisan vendor:publish --provider='Twedoo\\Volcator\\VolcatorServiceProvider' --tag=public --force"
        ]
    }
}
